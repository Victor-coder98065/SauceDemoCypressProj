{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["es2020", "dom"],
    "module": "ES2020",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["cypress", "@cypress/puppeteer/support"]
  },
  "include": ["cypress/**/*.ts"]
}